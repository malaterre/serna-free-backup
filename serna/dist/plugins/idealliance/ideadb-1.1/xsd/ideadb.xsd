  </xs:attributeGroup>
  <xs:simpleType name="notation.class">
    <xs:restriction base="xs:token">
      <xs:enumeration value="BMP"/>
      <xs:enumeration value="GIF"/>
      <xs:enumeration value="GIF87a"/>
      <xs:enumeration value="GIF89a"/>
